{
    "auth-sessions": {
        "login": {
            "method": "POST",
            "path": "/auth-sessions/email",
            "body": {
                "email": "string",
                "password": "string"
            },
            "response": {
                "user": "IUser",
                "accessToken": "string",
                "refreshToken": "string"
            }
        },
        "refresh": {
            "method": "POST",
            "path": "/auth-sessions/refresh",
            "body": {
                "refreshToken": "string"
            },
            "response": {
                "accessToken": "string"
            }
        }
    },
